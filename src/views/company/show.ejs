<%-include('../admin/includes/header') %>

    <div class="row">
        <div class="box box-success">
            <div class="box-header"><i class="fa fa-User"></i> Company </div>
            <div class="box-body">
                <form action="" method="post">
                    <form role="form" action="/company" method="post">
                        <div class="box-body">
          
                          
                          <div class="form-group">
                            <label for="company_name">Company Name</label>
                            <input type="text" class="form-control" id="company_name" name="company_name" placeholder="Enter company name" value="<%=list.company_name%>" autocomplete="off">
                          </div>
                          <div class="form-group">
                            <label for="service_charge_value">Charge Amount (%)</label>
                            <input type="text" class="form-control" id="service_charge_value" name="service_charge_value" placeholder="Enter charge amount %" value="<%=list.service_charge_value%>" autocomplete="off">
                          </div>
                          <div class="form-group">
                            <label for="vat_charge_value">Vat Charge (%)</label>
                            <input type="text" class="form-control" id="vat_charge_value" name="vat_charge_value" placeholder="Enter vat charge %" value="<%=list.vat_charge_value%>" autocomplete="off">
                          </div>
                          <div class="form-group">
                            <label for="address">Address</label>
                            <input type="text" class="form-control" id="address" name="address" placeholder="Enter address" value="<%=list.address%>" autocomplete="off">
                          </div>
                          <div class="form-group">
                            <label for="phone">Phone</label>
                            <input type="text" class="form-control" id="phone" name="phone" placeholder="Enter phone" value="<%=list.phone%>" autocomplete="off">
                          </div>
                          <div class="form-group">
                            <label for="country">Message</label>
                            <textarea class="form-control" id="message" name="message" placeholder="Enter Message"><%=list.message%></textarea>
                          </div>
                          
                        </div>
                        <!-- /.box-body -->
          
                        <div class="box-footer">
                          <button type="submit" class="btn btn-primary">Save Changes</button>
                        </div>
                      </form>
                </form>
            </div>
        </div>
    </div>

<%-include('../admin/includes/footer') %>
